<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.hmc.dao.FacilityDao">
	
	<select id="getAllFacility" resultType="com.hmc.vo.Facility">
		select
			facility as facility,
			identy_num as identyNum
		from
			t_facility
	</select>
</mapper>